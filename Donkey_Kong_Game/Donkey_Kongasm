#BOTAO PLAY
Button_Play:
	sw $31 0($29)
	addi $29 $29 -4
	
	addi $15 $2 0
	addi $16 $3 0
	
	addi $2 $0 60
	addi $3 $0 32
	addi $4 $15 0
	addi $5 $16 0
	addi $9 $0 0
	ori $9 0x874632
	jal Mario_Draw_Pixel
	
	addi $2 $0 64
	addi $3 $0 28
	addi $4 $15 -2
	addi $5 $16 2
	addi $9 $0 0
	ori $9 0x874632
	jal Mario_Draw_Pixel
	
	addi $2 $0 68
	addi $3 $0 24
	addi $4 $15 -4
	addi $5 $16 4
	addi $9 $0 0
	ori $9 0x874632
	jal Mario_Draw_Pixel
	
	addi $2 $0 72
	addi $3 $0 20
	addi $4 $15 -6
	addi $5 $16 6
	addi $9 $0 0
	ori $9 0x874632
	jal Mario_Draw_Pixel
	
	# AMARELO #BD961D
	addi $2 $0 60
	addi $3 $0 28
	addi $4 $15 0
	addi $5 $16 2
	addi $9 $0 0
	ori $9 0xBD961D
	jal Mario_Draw_Pixel
	
	# AMARELO MAIS FEIO #A78A24
