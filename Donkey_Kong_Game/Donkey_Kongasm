# PRAIA

Monkey:

    # =============================
    # EMPILHAR
    # =============================

	sw $31 0($29)
        addi $29 $29 -4


	sw $8 0($29)
         addi $29 $29 -4

	sw $7 0($29)
        addi $29 $29 -4

	sw $4 0($29)
         addi $29 $29 -4

	sw $3 0($29)
        addi $29 $29 -4

	sw $2 0($29)
        addi $29 $29 -4

	
    # =============================
    
    # =============================
    # SEGUIR DE BASE
    # =============================

	add $15 $7 $0
	add $16 $8 $0
	
    # =============================

	addi $2 $0 2	
	addi $3 $0 1
	addi $4 $15 5
	addi $5 $16 -5
	addi $9 $0 0
	ori $9 0xB4461B
	jal Monkey_Draw_Pixel
	
