        addi $29 $29 -4

       # sw $12 0($29)
       # addi $29 $29 -4

      #  sw $11 0($29)
       # addi $29 $29 -4

        sw $10 0($29)
        addi $29 $29 -4

        sw $9 0($29)
        addi $29 $29 -4

        sw $8 0($29)
        addi $29 $29 -4

        sw $7 0($29)
        addi $29 $29 -4

        sw $6 0($29)
        addi $29 $29 -4

        sw $5 0($29)
        addi $29 $29 -4

        sw $4 0($29)
        addi $29 $29 -4

        sw $3 0($29)
