timer: 
	sw $16, 0($29)
       	addi $29, $29, -4
       	addi $16, $0, 100000
forT:  	beq $16, $0, fimT
       	nop
       	nop
       	addi $16, $16, -1      
       	j forT                  
fimT:  	addi $29, $29, 4                                                    
       	lw $16, 0($29)          
       	jr $31
timer2: 
	sw $16, 0($29)
       	addi $29, $29, -4
       	addi $16, $0, 25000
forT2:  	
	beq $16, $0, fimT
       	nop
       	nop
       	addi $16, $16, -1      
       	j forT                  
fimT2:  
	addi $29, $29, 4                                                    
       	lw $16, 0($29)          
       	jr $31


Empilhar_all:
        sw $19 0($29)
        addi $29 $29 -4

        sw $18 0($29)
        addi $29 $29 -4

        sw $17 0($29)
        addi $29 $29 -4

        sw $16 0($29)
        addi $29 $29 -4

        sw $15 0($29)
        addi $29 $29 -4

        sw $14 0($29)
        addi $29 $29 -4

